{
  "name": "steward",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests --testPathPatterns=\"ReceiptUpload|LoginForm|ErrorBoundary|PageTransition\"",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests --maxWorkers=2 --testPathPatterns=\"ReceiptUpload|LoginForm|ErrorBoundary|PageTransition\"",
    "test:debug": "jest --verbose --no-cache --passWithNoTests",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:test": "dotenv -e .env.local -- node scripts/db-test.js",
    "validate:production": "node scripts/production-validation.js",
    "validate:env": "node scripts/production-validation.js --env-only",
    "validate:build": "node scripts/production-validation.js --build-only",
    "validate:test": "node scripts/production-validation.js --test-only",
    "validate:foundation": "node scripts/validate-foundation.js",
    "validate:foundation:local": "BASE_URL=http://localhost:3000 node scripts/validate-foundation.js",
    "validate:foundation:production": "BASE_URL=https://www.hellosteward.org node scripts/validate-foundation.js",
    "validate:foundation:production-compatible": "BASE_URL=https://www.hellosteward.org node scripts/validate-foundation-production.js",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:rollback": "./scripts/deploy-production.sh --rollback",
    "deploy:verify": "./scripts/deploy-production.sh --verify"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.2.0",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.3",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "openai": "^5.8.2",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^4.0.2",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
